{
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:@tanstack/eslint-plugin-query/recommended",
    "plugin:storybook/recommended"
  ],
  "plugins": ["prettier", "@tanstack/query", "@emotion"],
  "rules": {
    "prettier/prettier": "error",
    "react/no-unknown-property": ["error", { "ignore": ["css"] }]
  }
}
